{
    "pluginName": "Frontend Project Manager",
    "version": "0.1.8",
    "description": "Project Manager for frontend developers",
    "author": "Your Name",
    "homepage": "https://github.com/your/repo",
    "main": "index.html",
    "preload": "preload.js",
    "logo": "logo.png",
    "features": [
        {
            "code": "main",
            "explain": "Frontend Project Manager",
            "cmds": [
                "Frontend Project Manager",
                "fpm"
            ],
            "windowType": "system"
        },
        {
            "code": "import_project",
            "explain": "导入前端项目",
            "cmds": [
                {
                    "type": "files",
                    "label": "导入项目",
                    "fileType": "directory",
                    "match": "/.*/",
                    "minLength": 1
                }
            ],
            "windowType": "system"
        }
    ]
}
